function irParaJaneiro() {
  var aba = SpreadsheetApp.getActiveSpreadsheet().getSheetByName("Janeiro");
  if (aba) {
    aba.activate();
  } else {
    SpreadsheetApp.getUi().alert("A aba 'Janeiro' não foi encontrada.");
  }
}

